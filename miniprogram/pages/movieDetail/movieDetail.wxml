<!--pages/movieDetail/movieDetail.wxml-->
<view  wx:for="{{list}}" wx:key="index" style="margin-bottom:50rpx; border-bottom:1px solid #ddd;padding:30rpx;">
  <!-- 显示多张图片 item.fileid[] -->
  <text>{{index}}</text>
  <image wx:for="{{item.fileid}}" wx:key="index" src="{{item}}" style="width:200rpx;height:200rpx;"></image>
  <view>电影名称:{{item.name}}</view>
  <view>原因:{{item.msg}}</view>
  
</view>


